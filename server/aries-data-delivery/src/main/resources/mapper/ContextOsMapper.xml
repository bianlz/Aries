<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.bianlz.data.delivery.dao.ContextOsDao">
    <resultMap id="contextMap" type="cn.com.bianlz.data.delivery.api.vo.ContextOs">
        <id column="id" jdbcType="BIGINT" property="id" />
        <result column="uuid" jdbcType="BIGINT" property="uuid" />
        <result column="os" jdbcType="INTEGER" property="os" />
        <result column="date_stamp" jdbcType="VARCHAR" property="dateStamp" />
        <result column="create_date" jdbcType="TIMESTAMP" property="createDate" />
    </resultMap>
    <select id="getByUuid" resultMap="contextMap" >
          select * from ad_nadp_context_os where
            uuid = #{uuid,jdbcType=BIGINT}
            and
            date_stamp = #{date,jdbcType=VARCHAR}
    </select>
</mapper>
